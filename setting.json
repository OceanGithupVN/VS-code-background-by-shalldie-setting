// paste these into setting.json
"background.editor": {
          "useFront": true,
          "style": {
            "background-position": "100% 100%",
            "background-size": "auto",
            "opacity": 0.6// độ trong suốt của ảnh
          },
          "styles": [{}, {}, {}],
          // Local images can be dragged into the browser to quickly get the file protocol address from the address bar
          "images": ["https://pathtoimage.png", "file:///path/to/local/file"],// thêm ảnh ở màn hình chính
          "interval": 0,
          "random": false
        },
    "background.panel": {
      "images": [],// thêm ảnh thanh ngang (terminal) ở đây:
      "opacity": 0.1,
      "size": "cover",
      "position": "center",
      "interval": 0,
      "random": true
    },
    "background.sidebar": {
      "images": [],// thêm ảnh thanh dọc ở đây:
      "opacity": 0.1,
      "size": "cover",
      "position": "center",
      "interval": 0,
      "random": false
    }
